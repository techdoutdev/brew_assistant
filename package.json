{
    "name": "brew_assistant",
    "description": "A Node-RED home brewer assistant",
    "version": "0.1.0",
    "dependencies": {
        "node-red-contrib-sensor-ds18b20": "0.2.0",
        "node-red-contrib-stoptimer": "0.0.7",
        "node-red-contrib-unit-converter": "0.0.2",
        "node-red-dashboard": "3.1.3",
        "node-red-node-smooth": "0.1.2",
        "node-red-node-pi-gpio": "2.0.1"
    },
    "node-red": {
        "settings": {
            "flowFile": "flow.json",
            "credentialsFile": "flow_cred.json"
        }
